{
  "private": true,
  "name": "debtcollective",
  "description": "",
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "node": ">=0.10.33"
  },
  "scripts": {
    "build": "npm run svg && tarima -fdV",
    "lint": "eslint *.js src bin config models controllers middlewares",
    "repl": "node bin/repl.js",
    "test": "scripty",
    "test:unit": "scripty",
    "test:integration": "scripty",
    "utils:resetdb": "scripty",
    "utils:knexreset": "scripty",
    "report": "scripty",
    "start": "scripty",
    "iron-node": "scripty",
    "watch": "scripty",
    "svg": "svg-sprite -s --symbol-dest=svg --symbol-prefix=.svg- --ss=sprite.svg --si --sx --shape-id-generator=svg-%s --dest=views/includes public/svg/*.svg"
  },
  "tarima": {
    "bundle": true,
    "src": "src/**/*",
    "dest": "./public/build",
    "ignoreFiles": [
      ".gitignore"
    ],
    "filter": [
      "**/_entries/**"
    ],
    "rename": "**:{filepath/1/_entries}/{filename}.{extname}",
    "bundleOptions": {
      "extensions": {
        "js": "es6",
        "css": "post"
      },
      "rollup": {
        "format": "iife",
        "plugins": [
          "rollup-plugin-buble",
          "rollup-plugin-node-resolve",
          "rollup-plugin-commonjs"
        ],
        "rollup-plugin-node-resolve": {
          "module": true,
          "jsnext": true,
          "main": true,
          "browser": true
        }
      },
      "postcss": {
        "plugins": [
          "postcss-import",
          "postcss-cssnext"
        ]
      }
    }
  },
  "dependencies": {
    "aws-sdk": "^2.4.14",
    "basscss": "^8.0.2",
    "bcrypt-node": "^0.1.0",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "checkit": "^0.7.0",
    "cli-table": "^0.3.1",
    "colors": "^1.1.2",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "datasets-us-states-names": "^1.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.5",
    "gm": "^1.23.0",
    "im-metadata": "^3.0.0",
    "imagemagick-stream": "^4.1.1",
    "knex": "^0.11.9",
    "krypton-orm": "git://github.com/sgarza/krypton.git#dev",
    "lodash": "^4.14.2",
    "lodash-inflection": "^1.4.0",
    "marked": "^0.3.6",
    "method-override": "^2.3.6",
    "mkdirp": "^0.5.1",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "mysql": "^2.11.1",
    "neon": "^2.0.0",
    "nodemailer": "^2.5.0",
    "nodemailer-ses-transport": "^1.4.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^6.0.3",
    "pluralize": "^3.0.0",
    "pug": "^2.0.0-beta4",
    "redis": "^2.6.2",
    "req-flash": "0.0.3",
    "response-time": "^2.3.1",
    "rimraf": "^2.5.4",
    "route-mappings": "^0.3.1",
    "scandium-express": "0.0.3",
    "scripty": "^1.6.0",
    "uuid": "^2.0.2",
    "webfontloader": "^1.6.26",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "buble": "^0.12.5",
    "chai": "^3.5.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-mocha": "^4.3.0",
    "istanbul": "^0.4.4",
    "istanbul-combine": "^0.3.0",
    "mocha": "^3.0.2",
    "nodemailer-stub-transport": "^1.0.0",
    "postcss": "^5.1.1",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "rollup": "^0.34.7",
    "rollup-plugin-buble": "^0.12.1",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "superagent": "^2.1.0",
    "svg-sprite": "^1.3.3",
    "tarima-browser-sync": "^0.1.3",
    "tarima-cli": "^0.2.2",
    "testdouble": "^1.6.0"
  }
}
