{
  "private": true,
  "name": "debtcollective",
  "description": "",
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "node": ">=0.6"
  },
  "scripts": {
    "build": "scripty",
    "lint": "scripty",
    "repl": "node bin/repl.js",
    "test": "scripty",
    "test:unit": "scripty",
    "test:integration": "scripty",
    "utils:resetdb": "scripty",
    "utils:knexreset": "scripty",
    "utils:copybadges": "scripty",
    "migrate:dev": "knex migrate:latest --env development",
    "migrate:seed:dev": "knex seed:run --env development",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "report": "scripty",
    "start": "scripty",
    "start:dev": "nodemon --inspect=9220 bin/server.js",
    "iron-node": "scripty",
    "watch": "scripty",
    "dev": "tarima -wP localhost:3000 -p 3001 -e",
    "svg": "svg-sprite -s --symbol-dest=svg --symbol-prefix=.svg- --ss=sprite.svg --si --sx --shape-id-generator=svg-%s --dest=views/includes public/svg/*.svg",
    "post_deploy_staging": "scripty",
    "post_deploy_production": "./scripts/post_deploy_production",
    "disputes_sendto_admin": "./scripts/tasks/disputes-sendto-admin",
    "resend_activation_links": "./scripts/tasks/resend-activation-links",
    "prepush": "npm run lint"
  },
  "tarima": {
    "cwd": ".",
    "src": "src",
    "watch": "views",
    "dest": "./public/build",
    "ignoreFiles": [
      ".gitignore"
    ],
    "bundle": [
      "src/stylesheets/_entries/**"
    ],
    "filter": [
      "!node_modules/**",
      "!**/{components,lib}/**",
      "!**/{base,utils}*.css"
    ],
    "rename": [
      "**/_vendor/*:{filepath/1/_vendor}/{filename}.{extname}",
      "**/_entries/**:{filepath/1/_entries}/{filename}.{extname}"
    ],
    "devPlugins": [
      "tarima-lr"
    ],
    "bundleOptions": {
      "bundleCache": true,
      "entryCache": true,
      "extensions": {
        "css": "post"
      },
      "postcss": {
        "plugins": [
          "postcss-import",
          "postcss-cssnext"
        ]
      }
    }
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "archiver": "^1.1.0",
    "autosize": "^3.0.18",
    "aws-sdk": "^2.4.14",
    "basscss": "^8.0.2",
    "bcrypt-node": "^0.1.0",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "checkit": "^0.7.0",
    "chokidar": "^1.7.0",
    "cli-table": "^0.3.1",
    "colors": "^1.1.2",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "datasets-us-states-names": "^1.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.5",
    "gm": "^1.23.0",
    "hashids": "^1.1.4",
    "helmet": "^3.6.0",
    "humanize-num": "^1.1.5",
    "im-metadata": "^3.0.0",
    "imagemagick-stream": "^4.1.1",
    "jstransformer-verbatim": "^1.1.1",
    "knex": "^0.11.9",
    "krypton-orm": "git://github.com/sgarza/krypton.git#dev",
    "lodash": "^4.14.2",
    "lodash-inflection": "^1.4.0",
    "marked": "^0.3.6",
    "method-override": "^2.3.6",
    "mkdirp": "^0.5.1",
    "mmmagic": "^0.4.5",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "neon": "^2.0.0",
    "node-xlsx": "^0.7.4",
    "nodemailer": "^2.5.0",
    "nodemailer-mailgun-transport": "^1.3.5",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^6.0.3",
    "pisces": "0.0.15",
    "pluralize": "^3.0.0",
    "pug": "^2.0.0-beta4",
    "query-string": "^4.2.3",
    "raven": "^2.1.1",
    "redis": "^2.6.2",
    "req-flash": "0.0.3",
    "request": "^2.75.0",
    "response-time": "^2.3.1",
    "rimraf": "^2.5.4",
    "rollup-plugin-vue": "^3.0.0",
    "route-mappings": "^0.3.2",
    "sanitize-html": "^1.13.0",
    "scandium-express": "0.0.3",
    "scripty": "^1.6.0",
    "scroll-into-view-if-needed": "^1.0.2",
    "share-url": "^1.0.0",
    "stripe": "^4.14.0",
    "superagent": "^1.7.2",
    "uuid": "^2.0.2",
    "vue": "^2.5.9",
    "vue-multiselect": "^2.0.8",
    "vue-template-compiler": "^2.5.9",
    "webfontloader": "^1.6.26",
    "webpack": "^3.10.0",
    "winston": "^2.2.0",
    "winston-loggly-bulk": "^1.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.34",
    "@babel/preset-react": "^7.0.0-beta.34",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-jsx-v-model": "^2.0.3",
